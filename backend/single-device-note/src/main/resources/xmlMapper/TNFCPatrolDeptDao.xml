<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- mapper namespace作用是：代码和xml进行关联 -->
<!-- https://blog.csdn.net/xu962336414/article/details/105624371 -->
<mapper namespace="com.smlj.singledevice_note.logic.o.vo.table.dao.TNFCPatrolDeptDao">
    <select id="query" resultType="com.smlj.singledevice_note.logic.o.vo.table.entity.TNFCPatrolDept">
        select * from nfcpatrol.t_nfcpatrol_dept
        where id = #{id}
    </select>

    <select id="queryAll" resultType="com.smlj.singledevice_note.logic.o.vo.table.entity.TNFCPatrolDept">
        select * from nfcpatrol.t_nfcpatrol_dept where deleted = FALSE
    </select>
</mapper>
